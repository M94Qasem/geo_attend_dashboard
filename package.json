{
  "homepage": "https://themewagon.github.io/Notus-React",
  "name": "notus-react",
  "version": "1.1.0",
  "description": "Notus React - A free Tailwind CSS and React UI Kit and Admin by Creative Tim.",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.3",
    "@headlessui/react": "^2.2.8",
    "@heroicons/react": "^2.2.0",
    "@popperjs/core": "2.9.1",
    "@tanstack/react-table": "^8.21.3",
    "chart.js": "^4.5.0",
    "gulp": "^5.0.1",
    "gulp-append-prepend": "1.0.8",
    "leaflet": "^1.9.4",
    "papaparse": "^5.5.3",
    "react": "18.3.1",
    "react-datepicker": "^8.7.0",
    "react-dom": "18.3.1",
    "react-icons": "^5.5.0",
    "react-leaflet": "^4.2.1",
    "react-router": "5.3.4",
    "react-router-dom": "5.3.4",
    "react-scripts": "5.0.1",
    "react-select": "^5.10.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && gulp licenses",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run build:tailwind && npm start",
    "build:tailwind": "tailwindcss -i ./src/assets/styles/index.css -o ./src/assets/styles/tailwind.css --watch",
    "dev": "concurrently \"npm run build:tailwind\" \"npm start\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "typescript": "4.2.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@types/leaflet": "^1.9.20",
    "autoprefixer": "^10.2.0",
    "concurrently": "^9.2.1",
    "gh-pages": "^6.3.0",
    "postcss": "^8.2.0",
    "tailwindcss": "^3.0.0"
  }
}
